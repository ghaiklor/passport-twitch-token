{
  "name": "passport-twitch-token",
  "version": "0.0.0-semantic-release",
  "description": "Passport strategy for authenticating with Twitch via OAuth2 access tokens",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghaiklor/passport-twitch-token.git"
  },
  "keywords": [
    "passport",
    "twitch",
    "token",
    "auth",
    "authentication"
  ],
  "author": "ghaiklor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghaiklor/passport-twitch-token/issues"
  },
  "homepage": "https://github.com/ghaiklor/passport-twitch-token",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-add-module-exports": "1.0.1",
    "babel-preset-es2015": "6.24.0",
    "chai": "3.5.0",
    "chai-passport-strategy": "1.0.1",
    "coveralls": "3.0.9",
    "cz-conventional-changelog": "3.1.0",
    "isparta": "4.1.1",
    "mocha": "7.0.1",
    "semantic-release": "15.13.19",
    "sinon": "8.1.0"
  },
  "dependencies": {
    "passport-oauth": "1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "release": {
    "branch": "master"
  }
}
